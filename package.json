{
  "name": "solidity-template",
  "version": "1.0.0",
  "description": "solidity-template",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "size": "hardhat size-contracts",
    "build": "hardhat compile",
    "typechain": "hardhat typechain",
    "clean": "hardhat clean",
    "doc": "hardhat docgen",
    "lint": "tsc --noEmit -skipLibCheck && solhint 'src/**/*.sol' && prettier --check 'src/**/*.sol' 'helpers/**/*.ts' 'tasks/**/*.ts' 'hardhat.config.ts' 'helper-hardhat-config.ts'",
    "format": "prettier --write 'src/**/*.sol' 'helpers/**/*.ts' 'tasks/**/*.ts' 'hardhat.config.ts' 'helper-hardhat-config.ts'",
    "test": "hardhat test ./test/*.ts",
    "coverage": "hardhat coverage --testfiles 'test/*.ts'"
  },
